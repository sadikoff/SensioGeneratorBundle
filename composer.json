{
    "name": "sadikoff/flex-generator-bundle",
    "description": "This bundle generates code for you",
    "keywords": [],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        }
    ],
    "require": {
        "symfony/flex": "~1.0",
        "symfony/framework-bundle": "~3.3",
        "symfony/console": "~3.3",
        "symfony/process": "~3.3",
        "symfony/yaml": "~3.3",
        "twig/twig": "~2.0"
    },
    "require-dev": {
        "symfony/doctrine-bridge": "~3.3",
        "symfony/filesystem": "~3.3",
        "symfony/phpunit-bridge": "~3.3",
        "doctrine/orm": "~2.4"
    },
    "autoload": {
        "psr-4": { "Sensio\\Bundle\\GeneratorBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.1.x-dev"
        }
    }
}
